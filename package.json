{
  "name": "strt",
  "version": "1.0.0",
  "description": "Projeto de estudo para arquitetura node/typescript/express",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "test": "jest",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geonaoova/strt.git"
  },
  "keywords": [
    "node",
    "typescript",
    "express"
  ],
  "author": "Geovani Bertotti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/geonaoova/strt/issues"
  },
  "homepage": "https://github.com/geonaoova/strt#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "eslint-import-resolver-typescript": "^2.4.0",
    "express": "^4.17.1"
  }
}